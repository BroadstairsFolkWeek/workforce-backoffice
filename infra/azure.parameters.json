{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${{TEAMSFX_ENV}}"
    },
    "resourceBaseName": {
      "value": "wfbackoffice${{RESOURCE_SUFFIX}}"
    },
    "aadAppClientId": {
      "value": "${{AAD_APP_CLIENT_ID}}"
    },
    "aadAppClientSecret": {
      "value": "${{SECRET_AAD_APP_CLIENT_SECRET}}"
    },
    "aadAppTenantId": {
      "value": "${{AAD_APP_TENANT_ID}}"
    },
    "aadAppOauthAuthorityHost": {
      "value": "${{AAD_APP_OAUTH_AUTHORITY_HOST}}"
    },
    "wfApiUrl": {
      "value": "${{WF_API_URL}}"
    },
    "wfApiClientAuthAuthority": {
      "value": "${{WF_API_CLIENT_AUTH_AUTHORITY}}"
    },
    "wfApiClientAuthClientId": {
      "value": "${{WF_API_CLIENT_AUTH_CLIENT_ID}}"
    },
    "wfApiClientAuthClientSecret": {
      "value": "${{SECRET_WF_API_CLIENT_AUTH_CLIENT_SECRET}}"
    },
    "wfApiClientAuthScope": {
      "value": "${{WF_API_CLIENT_AUTH_SCOPE}}"
    },
    "groupId": {
      "value": "${{GROUP_ID}}"
    }
  }
}
